    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.5.3",
    "chai": "^4.1.2",
    "gts": "^0.8.0",
    "mocha": "^5.2.0",
    "supertest": "^3.1.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.3.1",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "express": "^4.16.3",
    "zipkin": "^0.13.0",
