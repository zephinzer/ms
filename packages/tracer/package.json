{
  "name": "@joeir/tracer",
  "version": "0.1.2",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --project .",
    "clean": "rm -rf ./dist/*",
    "lint": "tslint --config ../../tslint.yaml --project .",
    "prestart": "npm run build",
    "start": "ts-node test",
    "pretest": "docker-compose -f ./provisioning/deployment/compose/test.yml up -d",
    "test": "mocha --require ts-node/register \"./src/*.test.ts\" \"./src/**/*.test.ts\"",
    "posttest": "docker-compose -f ./provisioning/deployment/compose/test.yml down",
    "test:watch": "npm run test -- --watch --watch-extensions ts"
  },
  "publishConfig": {
    "access": "public"
  },
