{
  "name": "@joeir/accounts",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx babel-node --presets es2015 ./src -d ./dist",
    "dev": "npx nodemon src/index.js --exec ./node_modules/.bin/babel-node --presets @babel/env",
    "start": "node dist/index.js",
    "start-dev": "babel-node src/index.js --presets @babel/env",
    "db-exec": "docker exec -it accounts_db_mysql mysql -u user -ppassword",
    "db-update": "npx knex migrate:latest",
    "db-seed": "npx knex seed:latest",
    "migrations": "knex migrate:latest",
    "seeds": "knex seed:run",
    "provision": "docker-compose -f ./provisioning/compose/development/docker-compose.yml up"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.51",
    "@babel/node": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/preset-es2015": "^7.0.0-beta.52",
    "cross-env": "^5.2.0",
    "faker": "^4.1.0",
    "nodemon": "^1.17.5",
    "npx": "^10.2.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "convict": "^4.3.1",
    "crypto-js": "^3.1.9-1",
    "express": "^4.16.3",
    "knex": "^0.14.6",
    "mysql": "^2.15.0",
    "mysql2": "^1.5.3"
  }
}
